{
  "name": "spotlight-frontend",
  "version": "0.1.0",
  "engines": {
    "node": "11.1.0"
  },
  "private": true,
  "dependencies": {
    "connected-react-router": "^6.3.2",
    "express": "4.16.4",
    "g": "^2.0.1",
    "http-proxy-middleware": "0.19.1",
    "immutable": "^4.0.0-rc.12",
    "prop-types": "^15.7.2",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-drag-list": "^1.1.0",
    "react-redux": "^6.0.1",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "redux-immutable": "^4.0.0",
    "redux-observable": "^1.0.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.4.0",
    "rxjs-marbles": "^5.0.0",
    "styled-components": "^4.2.0"
  },
  "scripts": {
    "debug:server": "node --inspect ./build/server.js",
    "build:client": "react-scripts build",
    "build:server": "NODE_ENV=production webpack --mode production --config ./webpack/prod.server.js",
    "build": "npm run build:client && npm run build:server",
    "dev": "npm-run-all build:server --parallel debug:server start:localhost",
    "start": "node ./build/server.js",
    "start:localhost": "react-scripts start",
    "prod": "npm run build && npm run start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build -m \"[skip ci] Updates\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-react": "^7.12.4",
    "gh-pages": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  }
}
